{
    "name": "benny502/mini",
    "description": "mini frame",
    "type": "project",
    "authors": [
        {
            "name": "benny502",
            "email": "benny502@126.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3",
        "hassankhan/config": "^2.0",
        "symfony/yaml": "^4.3",
        "symfony/http-foundation": "^4.3",
        "symfony/routing": "^4.3",
        "doctrine/annotations": "^1.6",
        "symfony/http-kernel": "^4.3",
        "symfony/config": "^4.3",
        "illuminate/container": "^5.8",
        "symfony/debug": "^4.3",
        "twig/twig": "^2.11"

    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Mini\\": "frame/"
        },
        "files": [
            "frame/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
