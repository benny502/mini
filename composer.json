{
    "name": "benny502/mini",
    "description": "mini frame",
    "type": "project",
    "authors": [
        {
            "name": "benny502",
            "email": "benny502@126.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3",
        "hassankhan/config": "^2.0",
        "symfony/yaml": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Mini\\": "frame/"
        },
        "files": [
            "frame/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "app/tests/"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
